{
  "name": "fintech-example",
  "version": "0.0.0",
  "description": "Example of fintech project using Angular2, Typescript & Webpack build",
  "main": "src/main.ts",
  "scripts": {
    "reinstall": "rm -rf node_modules && npm i",
    "postinstall": "typings i",
    "start": "webpack-dev-server --history-api-fallback --env.TARGET DEV",
    "lint": "tslint src/**/*.ts",
    "test": "npm run lint",
    "build": "webpack --env.TARGET PROD",
    "ci": "npm test && npm run build",
    "prod": "npm run build && node server.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@ngrx/store": "1.5.0",
    "es6-shim": "0.35.0",
    "lodash": "4.12.0",
    "materialize-css": "0.97.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@ngrx/devtools": "^1.4.0",
    "autoprefixer": "^6.3.6",
    "clean-webpack-plugin": "^0.1.8",
    "compression-webpack-plugin": "^0.3.1",
    "connect": "^3.4.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^0.9.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "serve-static": "^1.10.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tslint": "^3.4.0",
    "typescript": "^1.9.0-dev.20160510",
    "typings": "^0.6.8",
    "url-loader": "^0.5.7",
    "watch": "^0.17.1",
    "webpack": "^2.1.0-beta.7",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomastrajan/fintech-example.git"
  },
  "keywords": [
    "fintech",
    "angular",
    "webpack",
    "typescript"
  ],
  "author": "Tomas Trajan <tomas.trajan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomastrajan/fintech-example/issues"
  },
  "homepage": "https://github.com/tomastrajan/fintech-example#readme"
}
